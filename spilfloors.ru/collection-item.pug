mixin typeOfMaterial(t, p)
	section.typeOfMaterial.section
		.container
			.section-title
				h2!= t
				p!= p
			.typeOfMaterial__inner
				.row
					block
mixin typeOfMaterial-item(c, l, t, i, i1, i2, i3, p)
	if c
		.col-4
			a.typeOfMaterial-item(href=`${l}`)
				img.typeOfMaterial-item__bg-img(src=`img/${i}`, alt="")
				.typeOfMaterial-item__hoverBlock
					h3!= t
					.typeOfMaterial-item__imgs
						img(src=`img/${i1}`, alt="")
						img(src=`img/${i2}`, alt="")
						img(src=`img/${i3}`, alt="")
					.typeOfMaterial-item__price!= p
	else
		a.typeOfMaterial-item(href=`${l}`)
			img.typeOfMaterial-item__bg-img(src=`img/${i}`, alt="")
			.typeOfMaterial-item__hoverBlock
				h3!= t
				.typeOfMaterial-item__imgs
					img(src=`img/${i1}`, alt="")
					img(src=`img/${i2}`, alt="")
					img(src=`img/${i3}`, alt="")
				.typeOfMaterial-item__price!= p
